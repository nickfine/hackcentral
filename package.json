{
  "name": "hackcentral",
  "private": true,
  "version": "0.6.41",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:backend & npm run dev:frontend",
    "dev:frontend": "vite",
    "dev:backend": "convex dev",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:perf:phase5": "vitest run tests/phase5-performance-harness.spec.ts",
    "test:coverage": "vitest run --coverage",
    "qa:phase3:macro-scaffold": "node scripts/phase3-macro-qa-scaffold.mjs",
    "qa:phase5:migration-dry-run": "node scripts/phase5-migration-dry-run.mjs",
    "qa:phase6:weekly-core": "npm run qa:phase5:migration-dry-run -- --environment production && npm run test:perf:phase5",
    "qa:phase6:telemetry-check": "cd forge-native && forge logs -e production --since 30m --limit 1000 | rg \"hdc-performance-telemetry|sync_execution|registry_lookup\" -n",
    "qa:phase6:weekly-check": "npm run qa:phase6:weekly-core && npm run qa:phase6:telemetry-check",
    "qa:phase7:weekly-report-scaffold": "node scripts/phase7-weekly-report-scaffold.mjs",
    "qa:phase7:full-width-backfill": "node scripts/phase7-full-width-backfill.mjs",
    "qa:spinout:template-smoke-scaffold": "node scripts/spinout-template-provision-smoke-scaffold.mjs",
    "qa:phase7:premerge": "npm --prefix forge-native run typecheck && npm run test:run",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "convex:env:set": "node scripts/set-convex-hackdays-env.mjs"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.60.0",
    "@internationalized/date": "^3.11.0",
    "@sentry/react": "^10.38.0",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "convex-helpers": "^0.1.111",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-aria-components": "^1.15.1",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  }
}
